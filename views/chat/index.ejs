<h2><%= header %></h2>

  <% userChats.forEach(function (chat) { %>
    <div class="well well-sm col-md-4">
      <div class="row">
        <div class="col-md-7">
        <%= chat.listing_id.name %><br>
        <%= chat.proposee_user_id.local.username %> <br>
        <strong>Initiated by: </strong><%= chat.proposer_user_id.local.username %> <br>
        </div>

        <div class="col-md-5">
        <a class="btn btn-primary view-message" href= <%= "/chats/mychats/" + chat._id %> >View Chat</a>
        </div>
      </div>
    </div>
  <% }) %>
