<h2>My Chats</h2>

<ul>
  <% userChats.forEach(function (chat) { %>
    <li>
      <strong>Chat ID: </strong><%= chat._id %> <br>
      <strong>Proposer UserID: </strong><%= chat.proposer_user_id %> <br>
      <strong>Proposee UserID: </strong><%= chat.proposee_user_id %> <br>
      <strong>Listing ID: </strong><%= chat.listing_id %> <br>
      <strong>Messages: </strong><%= chat.messages %> <br>
      <a href= <%= "/chats/mychats/" + chat._id %> >View Messages</a>
    </li>
  <% }) %>
</ul>
